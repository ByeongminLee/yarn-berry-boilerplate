{
  "name": "yarn-berry-boilerplate",
  "packageManager": "yarn@3.5.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "web:dev": "yarn workspace @yarn-berry-boilerplate/web run dev",
    "ui:dev": "yarn workspace @yarn-berry-boilerplate/ui storybook",
    "g:typecheck": "yarn workspaces foreach -pv run typecheck",
    "ui:new": "yarn hygen ui new"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "hygen": "^6.2.11"
  }
}
